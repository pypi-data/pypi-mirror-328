<!DOCTYPE html>
<html lang="{{request.locale_name}}">
  <head>
    <meta charset="utf-8">
    <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> -->
    <meta name="description" content="TANGOS: the agile numerical galaxy organization system">
    <meta name="author" content="TANGOS collaboration">
    <link rel="shortcut icon" href="{{request.static_url('tangos.web:static/icons/tangos-50x50.png')}}">

    <title>TANGOS</title>

      <link href="{{request.static_url('tangos.web:static/css/stylesheet.css')}}" rel="stylesheet">
      <link href="{{request.static_url('tangos.web:static/css/jquery-ui.min.css')}}" rel="stylesheet">


      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/sorttable.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/jquery-3.6.0.min.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/jquery-ui.min.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/autocomplete.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/dragtable.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/dynamictable.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/keeponscreen.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/pageupdate.js') }}"></script>
      <!-- NB formvalues.js must be included after dynamictable.js to make sure page elements are restored in correct order -->
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/formvalues.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/mergertree.js') }}"></script>
      <script type="text/javascript" src="{{ request.static_url('tangos.web:static/d3.min.js') }}"></script>
      <link href="https://fonts.googleapis.com/css?family=Quicksand:400,500,700" rel="stylesheet">
  </head>

  <body>

    <div class="content" id="content-container">
      <div id="breadcrumbs-top" class="dynamic-update">{{ request.breadcrumbs | safe }}</div>
          {% block content %}
            <div>No content!</div>
          {% endblock content %}
      <div id="breadcrumbs-bottom" class="dynamic-update">{{ request.breadcrumbs | safe }}</div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    </body>
</html>

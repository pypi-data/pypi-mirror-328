{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  // release.yml is generated by cargo-dist and should not be updated.
  "ignorePaths": [ ".github/workflows/release.yml" ],
  "prHourlyLimit": 10,
  schedule: ["before 4am on Monday"],
}

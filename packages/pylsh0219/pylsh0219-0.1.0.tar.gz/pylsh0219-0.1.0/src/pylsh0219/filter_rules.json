{
  "replacements": {
    "，": ",",
    "、": ",",
    "（": "(",
    "）": ")"
  },
  "remove_chars": ["*", "x", "×", " "],
  "regex_patterns": {
    "parentheses": "\\([^)]*\\)",
    "amount": "满[<i>]*(\\d+)[</i>]*"
  },
  "repeat_keywords": {
    "仅1次": ["仅可领1次", "仅可领取1次"],
    "3": ["仅可领3次", "仅可领取3次"],
    "10": ["共计可领取10次"]
  },
  "exclude_keywords": {
    "未知游戏": ["未知游戏"],
    "其他关键词": ["其他关键词1", "其他关键词2"]
  }
}
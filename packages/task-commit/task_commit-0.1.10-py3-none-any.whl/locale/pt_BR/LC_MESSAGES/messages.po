msgid "Error publishing branch"
msgstr "Erro ao publicar branch"

#: task_commit/utils.py:309
msgid "Error finalizing branch"
msgstr "Erro ao finalizar branch"

#: task_commit/utils.py:312
msgid "Invalid action"
msgstr "Ação inválida"

#: task_commit/utils.py:332
msgid "Error when pushing"
msgstr "Erro ao fazer push"


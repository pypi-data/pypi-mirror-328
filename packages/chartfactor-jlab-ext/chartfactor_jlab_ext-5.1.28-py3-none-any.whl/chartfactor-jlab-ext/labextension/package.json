{
  "name": "chartfactor_jlab_ext",
  "version": "5.1.28",
  "description": "Run chartfactor-py python commands in Jupyter Lab Kernels",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/Aktiun/chartfactor-jlab-ext",
  "bugs": {
    "url": "https://github.com/Aktiun/chartfactor-jlab-ext/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Aktiun",
    "email": "info@aktiun.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aktiun/chartfactor-jlab-ext.git"
  },
  "scripts": {
    "build": "jlpm run build:labextension:dev",
    "build:prod": "jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "clean:labextension": "rimraf chartfactor-jlab-ext/labextension",
    "clean:all": "jlpm run clean:labextension",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run build:prod",
    "prettier": "prettier --write \"**/*{.js,.jsx,.css,.json,.md}\"",
    "watch": "jlpm run watch:labextension",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyter-widgets/jupyterlab-manager": "^3.1.0",
    "@jupyterlab/application": "^3.2.9",
    "@jupyterlab/apputils": "^3.2.9",
    "@jupyterlab/docregistry": "^3.2.9",
    "@jupyterlab/notebook": "^3.2.9",
    "@phosphor/disposable": "^1.3.1",
    "jupyterlab_toastify": "^4.2.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "extension": true,
    "outputDir": "chartfactor-jlab-ext/labextension",
    "_build": {
      "load": "static/remoteEntry.9389ad178654549bbcb4.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "sharedPackages": {
    "@jupyter-widgets/base": {
      "bundled": false,
      "singleton": true
    }
  }
}

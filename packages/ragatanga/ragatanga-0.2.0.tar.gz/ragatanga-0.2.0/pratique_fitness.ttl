# Property Characteristics

# Transitive Property
:hasRequirement rdf:type owl:TransitiveObjectProperty .

# Functional Properties
:hasType rdf:type owl:FunctionalObjectProperty .
:isPoolOf rdf:type owl:FunctionalObjectProperty .

# Property Chains
:hasRequirement owl:propertyChainAxiom ( :hasPool :hasRequirement ) .
:offeredAt owl:propertyChainAxiom ( :hasPool :supportsActivity ) .

# Disjoint Properties
:associatedWith owl:propertyDisjointWith :operatesIn .

# Asymmetric Properties
:hasPool rdf:type owl:AsymmetricObjectProperty .
:includesBenefit rdf:type owl:AsymmetricObjectProperty .

# Irreflexive Properties
:hasPool rdf:type owl:IrreflexiveObjectProperty .
:includesBenefit rdf:type owl:IrreflexiveObjectProperty .
:isPoolOf rdf:type owl:IrreflexiveObjectProperty .

# Inverse Property Definition
:hasModalidade rdf:type owl:ObjectProperty ;
    owl:inverseOf :offeredAt ;
    rdfs:domain :Unidade ;
    rdfs:range :Modalidade ;
    rdfs:label "tem modalidade" ;
    rdfs:comment "Indica que a unidade oferece determinada modalidade" .


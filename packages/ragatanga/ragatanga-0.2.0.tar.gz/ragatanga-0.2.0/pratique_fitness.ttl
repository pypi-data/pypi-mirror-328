@prefix : <http://www.semanticweb.org/pratique-fitness#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:OntologyDeclaration rdf:type owl:Ontology ;
    rdfs:label "Pratique Fitness Ontology" .

# CLASSES

:Unidade rdf:type owl:Class ;
    rdfs:label "Unidade" ;
    rdfs:comment "Representa uma unidade física da Pratique Fitness." .

:UnidadeTipo rdf:type owl:Class ;
    rdfs:label "Tipo de Unidade" ;
    rdfs:comment "Classifica as unidades em: Slim, Premium e Prime/ Espaço Prime." .

:Plano rdf:type owl:Class ;
    rdfs:label "Plano / Assinatura" ;
    rdfs:comment "Modelos de assinatura e planos disponíveis para clientes." .

:Benefit rdf:type owl:Class ;
    rdfs:label "Benefício" ;
    rdfs:comment "Vantagens e serviços incluídos em cada plano." .

:PaymentMethod rdf:type owl:Class ;
    rdfs:label "Forma de Pagamento" ;
    rdfs:comment "Métodos de pagamento utilizados (ex.: cartão de crédito recorrente, PIX para parcelas vencidas)." .

:PratiqueNutriService rdf:type owl:Class ;
    rdfs:label "Serviço Pratique Nutri" ;
    rdfs:comment "Serviço de acompanhamento nutricional, via app Dietbox, com consulta inicial e acompanhamento periódico." .

:Modalidade rdf:type owl:Class ;
    rdfs:label "Modalidade" ;
    rdfs:comment "Atividades e modalidades oferecidas (musculação, dança, natação, etc.)." .

:Pool rdf:type owl:Class ;
    rdfs:label "Piscina" ;
    rdfs:comment "Informações sobre as piscinas disponíveis nas unidades (dimensões, tratamento, horários, etc.)." .

:BikeRental rdf:type owl:Class ;
    rdfs:label "Locação de Bike (PowerCycle)" ;
    rdfs:comment "Serviço de locação de bikes com planos (trimestral, quadrimestral, semestral) e benefícios inclusos." .

:PersonalExternoService rdf:type owl:Class ;
    rdfs:label "Serviço de Personal Externo" ;
    rdfs:comment "Programa para personal trainers atenderem alunos nas unidades, com taxas diferenciadas (geral, Prime e Itabira)." .

:PersonalClassService rdf:type owl:Class ;
    rdfs:label "Serviço de Personal Class" ;
    rdfs:comment "Programa de aulas personalizadas por meio de créditos, com avaliações mensais e agendamento via aplicativo." .

:JumperFitService rdf:type owl:Class ;
    rdfs:label "Serviço Jumper Fit" ;
    rdfs:comment "Recreação esportiva com trampolins, destinada a crianças, jovens e adultos, com planos mensais ou créditos avulsos." .

:ExameBioimpedancia rdf:type owl:Class ;
    rdfs:label "Exame de Bioimpedância" ;
    rdfs:comment "Exame que avalia a composição corporal (músculo, gordura, água, minerais) – incluso em vários planos ou disponível avulso por R$99,90." .

:History rdf:type owl:Class ;
    rdfs:label "História" ;
    rdfs:comment "Informações históricas e dados sobre o crescimento e a cultura da Pratique Fitness." .

# PROPRIEDADES DE OBJETO

:hasType rdf:type owl:ObjectProperty ;
    rdfs:domain :Unidade ;
    rdfs:range :UnidadeTipo ;
    rdfs:label "tem tipo" ;
    rdfs:comment "Relaciona uma unidade ao seu tipo (ex.: Slim, Premium, Prime)." .

:availableIn rdf:type owl:ObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :Unidade ;
    rdfs:label "disponível em" ;
    rdfs:comment "Indica as unidades em que um determinado plano está disponível." .

:hasPlan rdf:type owl:ObjectProperty ;
    owl:inverseOf :availableIn ;
    rdfs:domain :Unidade ;
    rdfs:range :Plano ;
    rdfs:label "has plan" ;
    rdfs:comment "Indica que a unidade possui o plano disponível." .

:includesBenefit rdf:type owl:ObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :Benefit ;
    rdfs:label "inclui benefício" ;
    rdfs:comment "Relaciona um plano aos benefícios que ele oferece." .

:isBenefitOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :includesBenefit ;
    rdfs:domain :Benefit ;
    rdfs:range :Plano ;
    rdfs:label "is benefit of" ;
    rdfs:comment "Indica que um benefício faz parte de um plano." .

:requiresPaymentMethod rdf:type owl:ObjectProperty ;
    rdfs:domain :Plano ;
    rdfs:range :PaymentMethod ;
    rdfs:label "requer forma de pagamento" ;
    rdfs:comment "Indica o método de pagamento utilizado para o plano." .

:isIncludedIn rdf:type owl:ObjectProperty ;
    rdfs:domain :PratiqueNutriService ;
    rdfs:range :Plano ;
    rdfs:label "está incluído em" ;
    rdfs:comment "Relaciona o serviço Nutri aos planos que o incluem." .

:offeredAt rdf:type owl:ObjectProperty ;
    rdfs:domain :Modalidade ;
    rdfs:range :Unidade ;
    rdfs:label "oferecido em" ;
    rdfs:comment "Indica as unidades onde a modalidade é oferecida." .

:associatedWith rdf:type owl:ObjectProperty ;
    rdfs:domain :Pool ;
    rdfs:range :Unidade ;
    rdfs:label "associado a" ;
    rdfs:comment "Relaciona uma piscina à unidade onde ela se encontra." .

:hasPool rdf:type owl:ObjectProperty ;
    owl:inverseOf :associatedWith ;
    rdfs:domain :Unidade ;
    rdfs:range :Pool ;
    rdfs:label "has pool" ;
    rdfs:comment "Indica que a unidade possui uma piscina associada." .

:operatesIn rdf:type owl:ObjectProperty ;
    rdfs:domain :PersonalExternoService ;
    rdfs:range :Unidade ;
    rdfs:label "opera em" ;
    rdfs:comment "Indica as unidades onde o serviço de Personal Externo pode ser utilizado." .

:isOperatedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :operatesIn ;
    rdfs:domain :Unidade ;
    rdfs:range :PersonalExternoService ;
    rdfs:label "is operated by" ;
    rdfs:comment "Indica que a unidade é operada pelo serviço de Personal Externo." .

:availableAtService rdf:type owl:ObjectProperty ;
    rdfs:domain :PersonalClassService ;
    rdfs:range :Unidade ;
    rdfs:label "disponível em" ;
    rdfs:comment "Indica as unidades onde o serviço de Personal Class é oferecido." .

:providesServiceAt rdf:type owl:ObjectProperty ;
    owl:inverseOf :availableAtService ;
    rdfs:domain :Unidade ;
    rdfs:range :PersonalClassService ;
    rdfs:label "provides service at" ;
    rdfs:comment "Indica que a unidade oferece o serviço de Personal Class." .

# PROPRIEDADES DE DADO

:hasPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:decimal ;
    rdfs:label "tem preço" ;
    rdfs:comment "Valor do plano em reais." .

:hasVariantPrice rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:decimal ;
    rdfs:label "tem preço variante" ;
    rdfs:comment "Preço alternativo do plano para unidades específicas." .

:hasUnitException rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "tem exceção de unidade" ;
    rdfs:comment "Especifica exceções de preços para unidades específicas." .

:hasRecurrence rdf:type owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "tem recorrência" ;
    rdfs:comment "Frequência do pagamento (Mensal, Semanal, Diária, etc.)." .

:hasBenefitDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain :Benefit ;
    rdfs:range xsd:string ;
    rdfs:label "descrição do benefício" ;
    rdfs:comment "Descrição detalhada do benefício oferecido." .

:hasPaymentComment rdf:type owl:DatatypeProperty ;
    rdfs:domain :PaymentMethod ;
    rdfs:range xsd:string ;
    rdfs:label "comentário sobre pagamento" ;
    rdfs:comment "Detalhes adicionais sobre a forma de pagamento." .

:hasLink rdf:type owl:DatatypeProperty ;
    rdfs:domain :ServiceUnion ;
    rdfs:range xsd:anyURI ;
    rdfs:label "tem link" ;
    rdfs:comment "Link para acesso, agendamento ou informações adicionais (ex.: agendamento do exame)." .

:hasAddress rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "possui endereço" ;
    rdfs:comment "Endereço completo da unidade." .

:hasCity rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "está na cidade" ;
    rdfs:comment "Cidade onde a unidade se encontra." .

:hasState rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "está no estado" ;
    rdfs:comment "Estado (UF) onde a unidade se encontra." .

:hasWorkingHours rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "horário de funcionamento" .

:hasPricingDetail rdf:type owl:DatatypeProperty ;
    rdfs:domain :Plano ;
    rdfs:range xsd:string ;
    rdfs:label "has pricing detail" ;
    rdfs:comment "Provides detailed pricing information for the plan, including unit-specific variations." .

:hasUnitPricingDetail rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:string ;
    rdfs:label "has unit pricing detail" ;
    rdfs:comment "Provides specific pricing variations for this unit as per KB information." .

# INDIVÍDUOS – Tipos de Unidade

:Tipo_Slim rdf:type :UnidadeTipo ;
    rdfs:label "Slim" ;
    rdfs:comment "Unidade com ticket médio entre R$129,90 e R$149,90. Exemplos: São Geraldo, Xangrilá, Pagani, etc." .

:Tipo_Premium rdf:type :UnidadeTipo ;
    rdfs:label "Premium" ;
    rdfs:comment "Unidade com acabamento e aparelhagem de alta qualidade. Exemplos: Trindade, Itacorubi, Chapecó, etc." .

:Tipo_Prime rdf:type :UnidadeTipo ;
    rdfs:label "Prime / Espaço Prime" ;
    rdfs:comment "Unidade com equipamentos superiores e atendimento premium (mais professores por aluno). Exemplos: Funcionários, Mangabeiras, Ouro Preto 2, etc." .

:Tipo_Slim owl:disjointWith :Tipo_Premium .
:Tipo_Slim owl:disjointWith :Tipo_Prime .
:Tipo_Premium owl:disjointWith :Tipo_Prime .


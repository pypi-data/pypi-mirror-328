    rdfs:range :Pool ;
    rdfs:label "possui piscina" ;
    rdfs:comment "Relaciona uma unidade à sua piscina" .

:isPoolOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasPool ;
    rdfs:domain :Pool ;
    rdfs:range :Unidade ;
    rdfs:label "piscina da unidade" .

# Helper Property
:hasSwimmingFacilities rdf:type owl:DatatypeProperty ;
    rdfs:domain :Unidade ;
    rdfs:range xsd:boolean ;
    rdfs:label "possui piscina" ;
    rdfs:comment "Indica se a unidade possui instalações de natação" .

